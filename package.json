{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production ESLINT=true webpack --config internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:nolint": "npm run clean && cross-env NODE_ENV=production ESLINT=false webpack --config internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:test": "cross-env NODE_ENV=production ENV_CONFIG=test ESLINT=false webpack --config internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:release": "cross-env NODE_ENV=production ENV_CONFIG=prod ESLINT=false webpack --config internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:prod": "cross-env NODE_ENV=production ENV_CONFIG=prod ESLINT=false webpack --config internals/webpack/webpack.config.prod.js --color -p --progress --hide-modules --display-optimization-bailout",
    "start": "cross-env NODE_ENV=development ENV_CONFIG=development node server",
    "start:test": "cross-env NODE_ENV=development ENV_CONFIG=test node server",
    "start:release": "cross-env NODE_ENV=release ENV_CONFIG=release node server",
    "start:production": "cross-env NODE_ENV=release ENV_CONFIG=release node server",
    "start:prod": "cross-env NODE_ENV=production node server"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "src/**/*.{css,less,scss}": [
      "npm run lint:style",
      "git add"
    ],
    "src/**/*.{json,html,css,less,scss,md,yaml,yml}": [
      "npm run lint:prettier",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "yarn run lint:js",
      "import-sort --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "react"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
